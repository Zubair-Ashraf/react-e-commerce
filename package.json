{
  "name": "react-e-commerce",
  "version": "1.0.0",
  "description": "React E-Commerce shopping cart app",
  "main": "index.js",
  "scripts": {
    "start:server": "cd server && env-cmd -f ./config/dev.env nodemon src/index.js --ext js,env --exec babel-node",
    "start:client": "cd client && react-scripts start",
    "start": "concurrently \"npm run start:server\" \"npm run start:server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zubair-Ashraf/react-e-commerce.git"
  },
  "keywords": [
    "reactjs",
    "nodejs",
    "express",
    "mongose"
  ],
  "author": "Zubair-Ashraf",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Zubair-Ashraf/react-e-commerce/issues"
  },
  "homepage": "https://github.com/Zubair-Ashraf/react-e-commerce#readme",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.21.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bootstrap": "^4.6.0",
    "chalk": "^4.1.1",
    "concurrently": "^6.2.0",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.7",
    "nodemon": "^2.0.7",
    "react": "^16.13.1",
    "react-bootstrap": "^1.6.0",
    "react-dom": "^16.13.1",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-toastify": "^7.0.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
